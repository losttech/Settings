<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <AssemblyName>LostTech.App.Settings</AssemblyName>
    <RootNamespace>LostTech.App</RootNamespace>
    <VersionPrefix>0.1</VersionPrefix>
    <Authors>Victor Milovanov</Authors>
    <Company>Lost Tech LLC</Company>
    <Product>LostTech.App</Product>
    <Description>Better way to store your app's settings. Key features:
- asynchronous
- autosave
- data bindable</Description>
    <RepositoryType>git</RepositoryType>
    <PackageTags>app settings async autosave</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="10.4.0" />
    <PackageReference Include="LostTech.Checkpoint" Version="0.1.0-b2017032402" />
    <PackageReference Include="PCLStorage.Abstractions" Version="[1.0.2,2.0)" />
    <PackageReference Include="RecursiveChangeNotifier" Version="0.3.0-b2017032602" />
  </ItemGroup>

</Project>